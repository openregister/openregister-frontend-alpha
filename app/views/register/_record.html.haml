%tr{class: "js-filter-item", "data-filter-terms" => record.instance_values.map { |key, value| value }.uniq }
  - @register_meta_data['register-record']['fields'].each do |field|
    %td{class: field}
      = record.instance_variable_get('@' + field.gsub('-', '_')) || "<span class='unknown'>No data found</span>".html_safe
